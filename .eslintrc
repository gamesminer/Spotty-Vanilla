{
  "extends": ["airbnb-base", "prettier"],
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "no-console": "error",
    "no-debugger": "error",
    "class-methods-use-this": "off",
    "import/prefer-default-export": "off",
    "import/no-extraneous-dependencies": "off",
    "no-new": "off",
    "no-return-assign": "off"
  }
}
